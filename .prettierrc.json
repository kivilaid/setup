{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "importOrder": [
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/types/(.*)$",
    "^@/utils/(.*)$",
    "^@/types/(.*)$",
    "^@/utils/(.*)$",
    "^@/components/(.*)$",
    "^@/components/ui/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports"]
}
